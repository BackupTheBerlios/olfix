	@mkdir -p .tmp/Saldolista && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/Saldolista/ && $(COPY_FILE) --parents frmsaldolista.ui.h .tmp/Saldolista/ && ( cd `dirname .tmp/Saldolista` && $(TAR) Saldolista.tar Saldolista && $(GZIP) Saldolista.tar ) && $(MOVE) `dirname .tmp/Saldolista`/Saldolista.tar.gz . && $(DEL_FILE) -r .tmp/Saldolista

