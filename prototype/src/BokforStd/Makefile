####### Compile

main.o: main.cpp Bokfstd.h

Bokfstd.h: Bokfstd.ui 
	$(UIC) Bokfstd.ui -o Bokfstd.h

Bokfstd.cpp: Bokfstd.h Bokfstd.ui 
	$(UIC) Bokfstd.ui -i Bokfstd.h -o Bokfstd.cpp

Bokfstd.o: Bokfstd.cpp Bokfstd.ui.h \
		Bokfstd.h

moc_Bokfstd.o: moc_Bokfstd.cpp Bokfstd.h 

