####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	test -d ../../bin/ || mkdir -p ../../bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: BokforStd.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile BokforStd.pro
qmake: 
	@$(QMAKE) -o Makefile BokforStd.pro

dist: 
	@mkdir -p .tmp/BOKFORSW && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/BOKFORSW/ && $(COPY_FILE) --parents Bokfstd.ui.h .tmp/BOKFORSW/ && ( cd `dirname .tmp/BOKFORSW` && $(TAR) BOKFORSW.tar BOKFORSW && $(GZIP) BOKFORSW.tar ) && $(MOVE) `dirname .tmp/BOKFORSW`/BOKFORSW.tar.gz . && $(DEL_FILE) -r .tmp/BOKFORSW

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
