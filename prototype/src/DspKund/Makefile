	@mkdir -p .tmp/DspKund && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/DspKund/ && $(COPY_FILE) --parents frmdspkund.ui.h .tmp/DspKund/ && ( cd `dirname .tmp/DspKund` && $(TAR) DspKund.tar DspKund && $(GZIP) DspKund.tar ) && $(MOVE) `dirname .tmp/DspKund`/DspKund.tar.gz . && $(DEL_FILE) -r .tmp/DspKund

