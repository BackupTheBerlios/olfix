dist: 
	@mkdir -p .tmp/LEVRESKW && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/LEVRESKW/ && $(COPY_FILE) --parents levresk.ui.h .tmp/LEVRESKW/ && ( cd `dirname .tmp/LEVRESKW` && $(TAR) LEVRESKW.tar LEVRESKW && $(GZIP) LEVRESKW.tar ) && $(MOVE) `dirname .tmp/LEVRESKW`/LEVRESKW.tar.gz . && $(DEL_FILE) -r .tmp/LEVRESKW

