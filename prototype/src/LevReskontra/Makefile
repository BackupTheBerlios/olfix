main.o: main.cpp levresk.h

levresk.h: levresk.ui 
	$(UIC) levresk.ui -o levresk.h

levresk.cpp: levresk.h levresk.ui 
	$(UIC) levresk.ui -i levresk.h -o levresk.cpp

