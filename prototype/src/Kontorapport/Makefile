main.o: main.cpp ktorapport.h

ktorapport.h: ktorapport.ui 
	$(UIC) ktorapport.ui -o ktorapport.h

ktorapport.cpp: ktorapport.h ktorapport.ui 
	$(UIC) ktorapport.ui -i ktorapport.h -o ktorapport.cpp

