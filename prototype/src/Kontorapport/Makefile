	@$(QMAKE) -o Makefile Kontorapport.pro

dist: 
	@mkdir -p .tmp/RPTKTOW && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/RPTKTOW/ && $(COPY_FILE) --parents ktorapport.ui.h .tmp/RPTKTOW/ && ( cd `dirname .tmp/RPTKTOW` && $(TAR) RPTKTOW.tar RPTKTOW && $(GZIP) RPTKTOW.tar ) && $(MOVE) `dirname .tmp/RPTKTOW`/RPTKTOW.tar.gz . && $(DEL_FILE) -r .tmp/RPTKTOW

