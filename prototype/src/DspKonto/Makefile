	@mkdir -p .tmp/DspKonto && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/DspKonto/ && $(COPY_FILE) --parents frmdspkonto.ui.h .tmp/DspKonto/ && ( cd `dirname .tmp/DspKonto` && $(TAR) DspKonto.tar DspKonto && $(GZIP) DspKonto.tar ) && $(MOVE) `dirname .tmp/DspKonto`/DspKonto.tar.gz . && $(DEL_FILE) -r .tmp/DspKonto

